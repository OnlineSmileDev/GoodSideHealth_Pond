  "scripts": {
    "start": "next start",
    "dev": "next dev",
    "start-pond-dev": "npm run pretty && concurrently --kill-others \"npm run prettier-watch\" \"npm run eslint-watch\" \"NEXT_PUBLIC_PROXY=https://pond.dev next dev\"",
    "start-pond-api": "npm run pretty && concurrently --kill-others \"npm run prettier-watch\" \"npm run eslint-watch\" \"NEXT_PUBLIC_PROXY=http://localhost:3040 next dev\"",
    "start-watch": "concurrently --kill-others \"npm run prettier-watch\" \"npm run eslint-watch\" \"npm run start\"",
    "build:development": "env-cmd -f .env.development next build",
    "build": "next build",
    "test": "jest",
    "test:e2e": "jest -c jest.e2e.config.js --runInBand",
    "test:e2e:debug": "PWDEBUG=1 jest -c jest.e2e.config.js --runInBand",
    "test:e2e:pond": "URL_BASE=https://pond.dev jest -c jest.e2e.config.js --runInBand",
    "test:cache": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:dd": "DD_ENV=ci DD_SERVICE=pond-ui npm run test:coverage",
    "test:e2e:dd": "DD_ENV=ci DD_SERVICE=pond-ui URL_BASE=https://pond.dev jest -c jest.e2e.config.js --runInBand",
    "format": "prettier -w .",
    "format:ci": "prettier -c .",
    "pretty": "npx prettier --write \"(src|e2e)/**/*.(js|css|ts)\"",
    "prettier-watch": "onchange \"(src|e2e)/**/*.(js|css)\" -- npx prettier --write {{changed}}",
    "lint": "eslint .",
    "eslint": "npm run lint",
    "eslint-watch": "onchange \"src/**/*.js\" -- npx eslint {{src}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "graphql:generate": "DOTENV_CONFIG_PATH=.env.local graphql-codegen -r dotenv/config --config codegen.yml",
    "graphql:generate-win": "graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
